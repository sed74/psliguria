<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/circle_solid" />
            <size
                android:width="@dimen/cross_circle_radius"
                android:height="@dimen/cross_circle_radius" />
            <stroke
                android:width="@dimen/circle_border_width"
                android:color="@color/circle_border" />
        </shape>
    </item>
    <item
        android:width="50dp"
        android:height="50dp"
        android:drawable="@drawable/helicopter_for_cross"
        android:gravity="center" />
</layer-list>